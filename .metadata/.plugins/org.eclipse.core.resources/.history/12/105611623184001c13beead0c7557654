import java.util.Locale;
import java.util.Scanner;

public class ExercicioIfElse8 {

	public static void main(String[] args) {
		
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		double salario = sc.nextDouble();
		double imposto;
		
		if(salario <= 2000.0 ) {
			imposto = 0.0;
		}

	}

}
